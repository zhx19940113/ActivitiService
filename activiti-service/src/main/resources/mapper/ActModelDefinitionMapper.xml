<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyb.commonactivity.mapper.ActModelDefinitionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dyb.commonactivity.entity.ActModelDefinition">
        <result column="id" property="id" />
        <result column="model_id" property="modelId" />
        <result column="model_code" property="modelCode" />
        <result column="model_name" property="modelName" />
        <result column="belong_system" property="belongSystem" />
        <result column="business_id" property="businessId" />
        <result column="model_desc" property="modelDesc" />
        <result column="status" property="status" />
        <result column="cre_user_id" property="creUserId" />
        <result column="cre_time" property="creTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, model_id AS modelId, model_code AS modelCode, model_name AS modelName, belong_system AS belongSystem, business_id AS businessId, model_desc AS modelDesc, status, cre_user_id AS creUserId, cre_time AS creTime
    </sql>

</mapper>
